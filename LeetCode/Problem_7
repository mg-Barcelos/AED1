bool judgeCircle(char * moves){
    int aux1 = 0, aux2 = 0;
    for(int i = 0; i < strlen(moves); i++){
        if(moves[i] == 'U'){
            aux1++;
        }
        else if(moves[i] == 'D'){
            aux1--;
        }
        else if(moves[i] == 'R'){
            aux2++;
        }
        else if(moves[i] == 'L'){
            aux2--;
        }
    }
    if(aux1 != 0 || aux2 != 0){
        return false;
    }

        return true;
}
